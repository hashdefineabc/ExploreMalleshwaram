@{
    ViewData["Title"] = "All Places";
}

@model IEnumerable<PlaceModel>

<div class="container">
    <h3 class="display-4">All Places</h3>

    <div class="row">


        @foreach (var place in Model)
        {
            <div class="col-md-4">
                
                <partial name="_placeThumbnail" model="@place"/>
            </div>
        }

    </div>
</div>
